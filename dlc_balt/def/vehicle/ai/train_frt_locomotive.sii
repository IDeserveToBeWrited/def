#----------------------------------------------------------------
# Finland
#----------------------------------------------------------------
traffic_vehicle : traffic.frt_fin
{
	accessories[]: .frt_fin.chassis
	accessories[]: .frt_fin.fwheel
	accessories[]: .frt_fin.rwheel

	max_speed: 140

	vehicle_mass: 72000
	gearbox[]: (0, 1)
	gearbox[]: (10, 0.9)
	gear_ratio: 10
	engine_torque: 35000

	trailer_chains[]: "train_car.frt_car_fi | train_car.frt_ca2_fi | train_car.frt_l_fin"

	displacement_limit: 0.0

	spawn_ratio: 0.0

	lod_dist[]: 70
	lod_dist[]: 130
	lod_dist[]: 200

}

vehicle_accessory: .frt_fin.chassis
{
	data_path: "/def/vehicle/ai/train_frt/chassis_l_fin.sii"
}

vehicle_wheel_accessory: .frt_fin.fwheel
{
	offset: 0
	data_path: "/def/vehicle/ai_wheel/front_empty_50cm.sii"
}

vehicle_wheel_accessory: .frt_fin.rwheel
{
	offset: 0
	data_path: "/def/vehicle/ai_wheel/rear_empty_50cm.sii"
}

