SiiNunit
{
# For modders: Please do not modify this file if you want to add a new entry. Create in
# this directory a new file "<base_name>.<idofyourmod>.sii" where <base_name> is name of
# base file without the extension (e.g. "city" for "/def/city.sii") and <idofyourmod> is
# some string which is unlikely to conflict with other mod.
#
# Warning: Even if the units are specified in more than one source file, they share the
# same namespace so suffixes or prefixes should be used to avoid conflicts.
#
# NOTE: To override a traffic info in your mod, just create a new definition
# in your definition file (see above) with the same value for 'object' attribute as the one being overridden
# For each 'object' the LAST LOADED traffic info is used
#

#################################################
########### car
#################################################

country_traffic_info : .country.info.traffic.toyco {
	object: traffic.toyco
	spawn_ratio: 1.00
}

country_traffic_info : .country.info.traffic.clio {
	object: traffic.clio
	spawn_ratio: 2.00
}

country_traffic_info : .country.info.traffic.laguna {
	object: traffic.laguna
	spawn_ratio: 1.00
}

country_traffic_info : .country.info.traffic.megane {
	object: traffic.megane
	spawn_ratio: 2.00
}

country_traffic_info : .country.info.traffic.p_3204_tr {
	object: traffic.p_3204_tr
	spawn_ratio: 1.00
}

country_traffic_info : .country.info.traffic.opalin_tr {
	object: traffic.opalin_tr
	spawn_ratio: 1.00
}


#################################################
########### police
#################################################


country_traffic_info : .country.info.traffic.toyco_p {
	object: traffic.toyco_p
	spawn_ratio: 1.00
}


country_traffic_info : .country.info.traffic.transport_t6.tr_customs {
	object: traffic.transport_t6.tr_customs
	spawn_ratio: 1.00
}


}
