traffic_vehicle : traffic.captur
{
	accessories[]: .captur.chassis
	accessories[]: .captur.fwheel
	accessories[]: .captur.rwheel

	max_speed: 160

	lod_dist[]: 60
	lod_dist[]: 110
	

	vehicle_mass: 1200
@include "drivers_generic.sui"	
}

vehicle_accessory: .captur.chassis
