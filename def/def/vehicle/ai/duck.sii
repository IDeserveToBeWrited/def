traffic_vehicle : traffic.duck
{
	accessories[]: .duck.chassis
	accessories[]: .duck.fwheel
	accessories[]: .duck.rwheel

	max_speed: 115

	lod_dist[]: 60
	lod_dist[]: 110
	
	vehicle_mass: 900
@include "drivers_generic.sui"
}

vehicle_accessory: .duck.chassis
{
	data_path: "/def/vehicle/ai/duck/chassis.sii"
}

vehicle_wheel_accessory: .duck.fwheel
{
	offset: 0
	data_path: "/def/vehicle/ai_wheel/duck_front.sii"
}

vehicle_wheel_accessory: .duck.rwheel
{
	offset: 0
	data_path: "/def/vehicle/ai_wheel/duck_rear.sii"
}