traffic_vehicle : traffic.ducato
{
	accessories[]: .ducato.chassis
	accessories[]: .ducato.fwheel
	accessories[]: .ducato.rwheel

	max_speed: 100

	lod_dist[]: 60
	lod_dist[]: 110
	

	vehicle_mass: 4000
@include "drivers_generic.sui"	
}

vehicle_accessory: .ducato.chassis
{
	data_path: "/def/vehicle/ai/ducato/drop_side.sii"
}

vehicle_wheel_accessory: .ducato.fwheel
{
	offset: 0
	data_path: "/def/vehicle/ai_wheel/27_steel_fiat_front.sii"
}

vehicle_wheel_accessory: .ducato.rwheel
{
	offset: 0
	data_path: "/def/vehicle/ai_wheel/27_steel_fiat_rear.sii"
}


